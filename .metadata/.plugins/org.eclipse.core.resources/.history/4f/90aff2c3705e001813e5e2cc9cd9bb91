package com.example.demo.practice;

public class Fibonacci {
	
	public static void main(String[] args) {
		System.out.println(fibonacci(9));
	}

	private static int fibonacci(int i) {
		int r =0;
		  if (i <= 1)
		       return i;
		  System.out.println();
		return  r = fibonacci(i-1)+fibonacci(i-2);
	}

}
