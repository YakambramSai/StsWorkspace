package com.example.demo.practice;

public class Fibonacci {
	
	public static void main(String[] args) {
		 for(int i=1; i<=9; i++){
			System.out.print(fibonacci(i) +" "); 
			}

	}

	private static int fibonacci(int i) {
		int r =0;
		  if (i <= 1)
		       return i;
		
		return   r= fibonacci(i-1)+fibonacci(i-2);
	}

}
